<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.resxt.stim.controllers.AppController" fx:id="root">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <TextField fx:id="login" styleClass="steam_textfield" />
    <Button id="btn_browse" fx:id="browseButton" maxHeight="22.0" minHeight="22.0" mnemonicParsing="false" prefHeight="22.0" styleClass="steam_button" text="..." />

    <Label fx:id="displayDataText"/>
    <Button text="Click to display data in the console" onAction="#onDisplayDataButtonClick"/>
</VBox>
