<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.ScrollPane?>
<?import java.net.URL?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.resxt.stim.controllers.SteamAppController" fx:id="root">
        <Button text="Go back to main menu" onAction="#onBackButtonClick" styleClass="steam_button"/>

        <Text fx:id="name">name</Text>
        <ImageView fx:id="headerImage"/>
        <Text fx:id="price">price</Text>
        <Text fx:id="releaseDate">releaseDate</Text>
        <Text fx:id="positiveRecommendations">positiveRecommendations</Text>
        <Text fx:id="descriptionShort">descriptionLong</Text>
        <Text fx:id="supportedLanguages">supportedLanguages</Text>
        <Text fx:id="developers">developers</Text>
        <Text fx:id="publishers">publishers</Text>
        <Text fx:id="pcRequirementsMinimum">pcRequirementsMinimum</Text>
        <Text fx:id="pcRequirementsRecommended">pcRequirementsRecommended</Text>
        <Text fx:id="website">website</Text>
        <Text fx:id="supportUrl">supportUrl</Text>
        <Text fx:id="supportEmail">supportEmail</Text>

        <stylesheets>
                <URL value="@style.css" />
        </stylesheets>
</VBox>
